{
    "check-coverage": true,
    "extension": [
        ".js"
    ],
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "src/index.js"
    ],
    "reporter":[
        "html"
    ],
    "thresholds": {
        "global": {
            "lines": 85,
            "functions": 85,
            "statements": 85
        }
    },
    "all": true
}